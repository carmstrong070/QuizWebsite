{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Customizable": {
    // Name of the cookie used for authentication
    // Required, otherwise exception will occur
    "AuthenticationCookieName": "quizWebsiteCookie",

    // Time in minutes until cookie expires
    // Required, otherwise exception will occur
    "CookieTimeOutDurationInMinutes": "60"
  }
}
